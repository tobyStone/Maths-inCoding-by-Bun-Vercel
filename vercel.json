{
  "version": 2,
  "rewrites": [
    {
      "source": "/:prefix*/videoPlayer/:slug*",
      "destination": "/api/videoPlayer.js?prefix=:prefix&slug=:slug"
    },
    {
      "source": "/:prefix*/maths_questions/:slug*",
      "destination": "/api/maths_questions.js?prefix=:prefix&slug=:slug"
    },
    {
      "source": "/submit-feedback",
      "destination": "/api/submit_feedback.js"
    },
    {
      "source": "/api/chat",
      "destination": "/api/maths_questions.js"
    },
    {
      "source": "/api/cosine_similarity",
      "destination": "/api/cosine_similarity.js"
    },
    {
      "source": "/api/handle_questions",
      "destination": "/api/handle_questions.js"
    },
    {
      "source": "/api/login",
      "destination": "/api/login.js"
    },
    {
      "source": "/teacher_dashboard",
      "destination": "/api/teacher_dashboard.js"
    },
    {
      "source": "/repeat_teacher_dashboard",
      "destination": "/api/repeat_teacher_dashboard.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/general_page.js?path=$1"
    }
  ]
}